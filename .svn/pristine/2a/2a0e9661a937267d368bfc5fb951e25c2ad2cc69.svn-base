    public class OrderedProducts implements Serializable {

        private String productId;
        private String catId;
        private String price;
        private String productQty;
        private String addOnParentProductId;
        private String productName;
        private String productImage;
        private String vat;
        private String swatchBharatTax;
        private String serviceTax;
        private String instructions;
        private String description;
        private String productType;
        private String discount;
        private boolean isAddOn;
        private ArrayList<OrderedProducts> orderedProductList;

        public OrderedProducts(String productId, String catId, String price, String productQty,
                               String addOnParentProductId, String productName, String productImage,
                               String vat, String swatchBharatTax, String serviceTax, boolean isAddOn,
                               String instructions, String descriptionString, String productType,
                               String discount) {
            this.productId = productId;
            this.catId = catId;
            this.price = price;
            this.productQty = productQty;
            this.addOnParentProductId = addOnParentProductId;
            this.productName = productName;
            this.productImage = productImage;
            this.vat = vat;
            this.swatchBharatTax = swatchBharatTax;
            this.serviceTax = serviceTax;
            this.isAddOn = isAddOn;
            this.instructions = instructions;
            this.description = descriptionString;
            this.productType = productType;
            this.discount = discount;
        }

        public OrderedProducts(JSONArray jsonArray) {
            try {

                orderedProductList = new ArrayList<>();

                for (int i = 0; i < jsonArray.length(); i++) {
                    JSONObject object = jsonArray.getJSONObject(i);

                    OrderedProducts orderedProducts = new OrderedProducts
                            (object.getString("product_id"),
                            object.getString("cat_id"),
                            object.getString("price"),
                            object.getString("product_qty"),
                            object.getString("addon_parent_product_id"),
                            object.getString("product_name"),
                            object.getString("product_img"),
                            object.getString("vat"),
                            object.getString("swach_bharat_tax"),
                            object.getString("service_tax"),
                            object.getBoolean("is_addon"),
                            object.getString("instruction"),
                            object.getString("discription"),
                            object.getString("product_type"),
                            object.getString("discount"));

                    orderedProductList.add(orderedProducts);
                }

                setOrderedProductList(orderedProductList);

            } catch (Exception e) {
                e.printStackTrace();
            }
        }

        public String getInstructions() {
            return instructions;
        }

        public void setInstructions(String instructions) {
            this.instructions = instructions;
        }

        public String getDescription() {
            return description;
        }

        public void setDescription(String description) {
            this.description = description;
        }

        public String getProductType() {
            return productType;
        }

        public void setProductType(String productType) {
            this.productType = productType;
        }

        public String getDiscount() {

    public OrderHistory(JSONObject jsonObject) {
        try {

            if (jsonObject.getInt("status") == 1) {
                orderHistoryList = new ArrayList<>();
                JSONArray jsonArray = jsonObject.getJSONArray("order_history");
                if (jsonArray.length() > 0) {
                    for (int i = 0; i < jsonArray.length(); i++) {
                        JSONObject jo = jsonArray.getJSONObject(i);

                        OrderedProducts orderedProducts = new OrderedProducts(jo.getJSONArray("products"));

                        OrderHistory orderHistory = new OrderHistory(jo.getString("order_id"),              // orderId;
                                jo.getString("order_date"),                     // orderDate;
                                jo.getString("order_time"),                     // orderTime;
                                jo.getString("month"),                          // orderMonth;
                                jo.getString("order_status"),                   // orderStatus;
                                jo.getString("total_item"),                     // orderItemTotal;
                                jo.getString("total_amount"),                   // orderAmountTotal;
                                jo.getString("orderlist"),                      // orderlist;
                                jo.getString("address_id"),                     // orderAddressId;
                                orderedProducts.getOrderedProductList(),        // orderedProductList;
                                jo.getString("email"),                          // orderEmail;
                                jo.getString("biller_name"),                    // orderBillerName;
                                jo.getString("branch_id"),                      // orderBranchId;
                                jo.getString("branch_name"),                    // orderBranchName;
                                jo.getString("branch_address"),                 // orderBranchAddress;
                                jo.getString("inprocess_time"),                 // orderInprocessTime;
                                jo.getString("acceptance"),                     // orderAcceptance;
                                jo.getString("on_road"),                        // orderOnRoad;
                                jo.getString("onroad_time"),                    // orderOnRoadTime;
                                jo.getString("delivery"),                       // orderOrderDelivery;
                                jo.getString("delivery_time"),                  //  orderOrderDeliveryTi
                                jo.getString("decline"),                        // orderDecline;
                                jo.getString("push_notification"),              // orderPushNotification
                                jo.getString("imei"),                           // orderImei;
                                jo.getString("discounted_amount"),              // orderDiscountAmount;
                                jo.getString("actual_amount"),                  // orderActualAmount;
                                jo.getString("coupon_code"),                    // orderCoupanCode;
                                jo.getString("neworder_on_mobile"),             // orderNewOrderOnMobile
                                jo.getString("tax_amount"),                     // orderTaxAmount;
                                jo.getString("delivery_phone"),                 // orderDeliveryPhone;
                                jo.getString("order_instruction"),              // orderInstruction;
                                jo.getString("service_tax_amount"),             // orderServiceTaxAmount
                                jo.getString("vat_amount"),                     // orderVatAmount;
                                jo.getString("surcharge_amount"),               // orderSurChargeAmount;
                                jo.getString("take_away"),                      // orderTakeAway;
                                jo.getString("delivery_charge"),                // orderDeliveryCharge;
                                jo.getString("service_charge"),                 // orderServiceCharge;
                                jo.getString("decline_reason"),                 // orderDeclineReason;
                                jo.getString("payment_mode"),                   // orderPaymentMode;
                                jo.getString("STATUS"),                         // orderPAYTMSTATUS;
                                jo.getString("TXNID"),                          // orderPAYTMTXNID;
                                jo.getString("TXNID"),                          // orderPAYTMORDERID;
                                jo.getString("CURRENCY"),                       // orderPAYTMCURRENCY;
                                jo.getString("BANKTXNID"),                      // orderPAYTMBANKTXNID;
                                jo.getString("TXNDATE"),                        // orderPAYTMTXNDATE;
                                jo.getString("BANKNAME"),                       // orderPAYTMBANKNAME;
                                jo.getString("CHECKSUMHASH"),                   // orderPAYTMCHECKSUMHA
                                jo.getString("MID"),                            // orderPAYTMMID;
                                jo.getString("RESPCODE"),                       // orderPAYTMRESPCODE;
                                jo.getString("RESPMSG"),                        // orderPAYTMRESPMSG;
                                jo.getString("PAYMENTMODE"),                    // orderPAYTMMODE;
                                jo.getString("TXNAMOUNT"));                     // orderPAYTMTXNAMOUNT;

                        orderHistoryList.add(orderHistory);
                    }
                }
            } else {
                setErrorMessage(jsonObject.getString("message"));
            }

        } catch (Exception e) {
            e.printStackTrace();
            errorMessage = "There is some problem in our server. Please try after sone time.";
        }
    }

